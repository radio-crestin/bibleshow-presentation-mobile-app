{
  "name": "bibleshow-remote",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:tunnel": "expo start --tunnel",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "dotenv -- expo run:android",
    "ios": "dotenv -- expo run:ios",
    "ios:device": "dotenv -- expo run:ios --device",
    "android:device": "dotenv -- expo run:android --device",
    "build:ios-remote": "dotenv -- eas build --platform ios",
    "build:ios-remote-development": "dotenv -- eas build --platform ios --profile preview",
    "submit-to-appstore:ios": "dotenv -- eas submit --platform ios ",
    "build:ios-locally": "dotenv -- eas build --platform ios --profile preview --local --output ./builds/ios/app.ipa",
    "build:android-locally": "dotenv -- eas build --platform android --profile preview --local --output ./builds/android/app.apk",
    "web": "dotenv -- expo start --web",
    "doctor": "npx expo-doctor",
    "test": "jest --watchAll",
    "lint": "expo lint"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/cli": "^15.1.3",
    "@react-native-picker/picker": "^2.11.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@sentry/react-native": "~6.3.0",
    "eas-cli": "^15.0.9",
    "expo": "~52.0.35",
    "expo-blur": "~14.0.3",
    "expo-brightness": "~13.0.3",
    "expo-constants": "~17.0.5",
    "expo-dev-client": "~5.0.12",
    "expo-font": "~13.0.3",
    "expo-haptics": "~14.0.1",
    "expo-image": "^2.0.5",
    "expo-keep-awake": "^14.0.2",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.17",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.2",
    "expo-system-ui": "~4.0.8",
    "expo-web-browser": "~14.0.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "react-native-wheel-color-picker": "^1.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "dotenv-cli": "^8.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.4",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.3.3"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
